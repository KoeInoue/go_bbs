{
  "name": "Go-Practice",
	"dockerComposeFile": [
		"../docker-compose.yml",
	],
	"service": "web",
	"workspaceFolder": "/go/src/github.com/KoeInoue/viet_college_api",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/ash",
		"go.toolsManagement.checkForUpdates": "off",
		"go.gopath": "/go",
		"go.gocodeAutoBuild": true,
		"go.formatTool": "gofmt",
		"go.useLanguageServer": true,
		"editor.formatOnSave": false,
		"[go]": {
			"editor.formatOnSave": true
		}
	},
	"extensions": [
		"golang.go"
	],
}
